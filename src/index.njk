---
layout: default
eleventyExcludeFromCollections: true
name: datashaman
---
<section>
  {% for page in collections.all %}
  <article class="pure-g">
    <div class="pure-u-1 pure-u-lg-1-8">
      <a href="{{ page.url }}"><time>{{ page.date | date('yyyy/MM/dd HH:mm') }}</time></a>
    </div>

    <div class="pure-u-1 pure-u-lg-7-8">
      {% autolink %}
        {% if page.data.layout === 'note' %}
          {{ page.templateContent | safe }}
        {% else %}
          {{ page.data.name }}
        {% endif %}
      {% endautolink %}
    </div>
  </article>
  {% endfor %}
</section>
